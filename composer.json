{
  "name": "itinerisltd/gfe-preset",
  "type": "wordpress-muplugin",
  "description": "Utilities for Gravity Forms Encrypted Fields.",
  "keywords": [
    "encryption",
    "gravity-forms",
    "gravityforms",
    "security",
    "wordpress",
    "wp"
  ],
  "homepage": "https://itinerisltd.github.io/gfe-preset/",
  "license": "MIT",
  "authors": [
    {
      "name": "Itineris Limited",
      "email": "hello@itineris.co.uk",
      "homepage": "https://itineris.co.uk/",
      "role": "Company"
    },
    {
      "name": "Tang Rufus",
      "email": "tangrufus@gmail.com",
      "homepage": "https://typist.tech/",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "/Users/lee-work/Code/misc/itineris-wp-coding-standards"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "itinerisltd/itineris-wp-coding-standards": "dev-php-8",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Itineris\\GFEPreset\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "pretag": [
      "composer update",
      "@style:check",
      "composer normalize",
      "npx doctoc README.md",
      "github_changelog_generator --no-verbose"
    ],
    "style:check": "phpcs",
    "style:fix": "phpcbf"
  },
  "support": {
    "email": "hello@itineris.co.uk",
    "issues": "https://github.com/ItinerisLtd/gfe-preset/issues",
    "source": "https://github.com/ItinerisLtd/gfe-preset"
  }
}
